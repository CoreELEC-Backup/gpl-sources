<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.gw"
		name="Handheld Electronic (GW)"
		version="1.6.3.6"
		provider-name="Libretro">
	<requires>
		<import addon="game.libretro" version="1.0.0"/>
	</requires>
	<extension point="kodi.gameclient"
			library_@PLATFORM@="@LIBRARY_FILENAME@">
		<platforms></platforms>
		<extensions>mgw</extensions>
		<supports_vfs>true</supports_vfs>
		<supports_standalone>false</supports_standalone>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en_GB">Handheld Electronic (GW) 1.6.3</summary>
		<description lang="en_GB">GW is a Game &amp; Watch simulator.

Supported files: .mgw</description>
		<license>zlib</license>
		<platform>@PLATFORM@</platform>
		<source>https://github.com/kodi-game/game.libretro.gw</source>
		<assets>
			<screenshot>resources/screenshot-01.jpg</screenshot>
			<screenshot>resources/screenshot-02.jpg</screenshot>
			<screenshot>resources/screenshot-03.jpg</screenshot>
		</assets>
	</extension>
</addon>

#: ps/help.c:114
msgid " -p, p, --pid <PID>   process id\n"
msgstr " -p, p, --pid <PID>   mã số tiến trình\n"

#: ps/help.c:115
msgid "        --ppid <PID>  parent process id\n"
msgstr "        --ppid <PID>  mã số tiến trình cha\n"

#: ps/help.c:116

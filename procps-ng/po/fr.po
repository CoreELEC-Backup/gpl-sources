msgstr " -l                   format long\n"

#: ps/help.c:136
msgid "  l                   BSD long format\n"
msgstr "  l                   format long BSD\n"

#: ps/help.c:137
msgid " -M, Z                add security data (for SELinux)\n"
msgstr " -M, Z                ajoute les données de sécurité (pour SELinux)\n"


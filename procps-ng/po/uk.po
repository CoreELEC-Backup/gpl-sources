msgid " -c, --read-rc               read the default rc\n"
msgstr " -c, --read-rc               прочитати типовий rc\n"

#: pmap.c:117
msgid " -C, --read-rc-from=<file>   read the rc from file\n"
msgstr " -C, --read-rc-from=<файл>   прочитати rc з файла\n"


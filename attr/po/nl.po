#: tools/setfattr.c:180
#, c-format
msgid "%s %s -- set extended attributes\n"
msgstr "%s %s -- zet uitgebreide attributen\n"

#: tools/setfattr.c:181 tools/getfattr.c:380
#, c-format
msgid "Usage: %s %s\n"
